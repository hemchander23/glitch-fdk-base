{
  "username": {
    "display_name": "Username",
    "description": "Please enter your username",
    "type": "text",
    "required": true,
    "regex": {
      "name": "^[a-zA-Z]*$",
      "name-error": "Name can only contain alphabets",
      "length": "^[a-zA-Z]{1,25}$",
      "length-error": "Length within only 1 and 25 only"
    }
  },
  "country": {
    "display_name": "Country",
    "description": "Please enter your country",
    "type": "dropdown",
    "options": [
      "India",
      "Iceland",
      "US",
      "UK"
    ],
    "default_value": "US"
  }
}
