{
  "name": "fdk",
  "version": "5.0.7",
  "description": "Freshworks Development Kit",
  "main": "index.js",
  "author": "Subhash",
  "license": "Apache-2.0",
  "dependencies": {
    "adm-zip": "^0.4.11",
    "ajv": "4.11.8",
    "archiver": "^1.0.0",
    "async": "^2.1.4",
    "body-parser": "^1.15.0",
    "cheerio": "0.22.0",
    "eslint": "4.19.1",
    "esprima": "^4.0.1",
    "express": "^4.13.4",
    "express-ws": "^3.0.0",
    "faker": "4.1.0",
    "fs-extra": "1.0.0",
    "image-size": "0.5.1",
    "inquirer": "^3.2.0",
    "istanbul": "^0.4.5",
    "istanbul-lib-instrument": "3.3.0",
    "jquery-deferred": "0.3.1",
    "json-schema-faker": "0.5.0-rc16",
    "lodash": "4.17.10",
    "moment-timezone": "^0.5.14",
    "node-dir": "0.1.16",
    "nodetree": "^0.0.3",
    "npm": "4.1.1",
    "passport": "^0.3.2",
    "passport-oauth2": "^1.3.0",
    "passport-oauth2-refresh": "^1.0.0",
    "request": "2.88.0",
    "sync-request": "5.0.0",
    "uuid": "^3.1.0",
    "validator": "^9.0.0",
    "wiz-cliparse": "^0.5.4"
  },
  "preferGlobal": true,
  "scripts": {
    "lint": "eslint -f table .",
    "code-sanity": "yarn check --integrity && yarn lint",
    "generate-version": "./node_modules/gulp/bin/gulp.js generate-version",
    "generate-addon": "./node_modules/gulp/bin/gulp.js generate-addon-zip",
    "generate-fdk": "./node_modules/gulp/bin/gulp.js production && npm pack",
    "test": "yarn unit-tests",
    "unit-tests": "NODE_ENV=test nyc mocha test/*.js -t 6000",
    "remote-parity": "git fetch && [ \"$(git rev-parse HEAD)\" == \"$(git rev-parse @{u})\" ]",
    "precommit": "yarn install && yarn code-sanity && yarn test && yarn remote-parity"
  },
  "bin": {
    "fdk": "./index.js"
  },
  "eslintConfig": {
    "extends": "eslint-config-freshapps",
    "globals": {
      "__debug": true,
      "APP_SOURCE": true,
      "PLATFORM_SOURCE": true,
      "jQuery": true,
      "window": true,
      "nukeCache": true
    },
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "rules": {
      "no-console": 0,
      "global-require": 0,
      "complexity": 0,
      "max-nested-callbacks": 0,
      "init-declarations": 0
    },
    "overrides": {
      "files": [
        "test/**/*",
        "tests/**/*",
        "integration/**/*"
      ]
    }
  },
  "eslintIgnore": [
    "views/",
    "coverage/",
    "test/",
    "test-res/",
    "addon/",
    "extras/"
  ],
  "private": true,
  "devDependencies": {
    "chai": "^4.1.2",
    "gulp": "^4.0.0",
    "gulp-replace": "^0.5.4",
    "mocha": "^5.0.0",
    "nock": "^9.1.4",
    "nyc": "^11.2.1",
    "sinon": "6.3.5",
    "supertest": "^3.0.0",
    "tmp": "^0.0.33",
    "websocket": "^1.0.25"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 82,
    "reporter": [
      "clover",
      "html",
      "text-summary",
      "json"
    ],
    "exclude": [
      "gulpfile.js",
      "template",
      "index.js",
      "test-res",
      "lib/web/assets/**",
      "test/**"
    ]
  }
}
